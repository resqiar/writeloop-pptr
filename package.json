{
  "name": "writeloop-pptr",
  "version": "1.0.0",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -p 3033 dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resqiar/writeloop-pptr.git"
  },
  "keywords": [],
  "author": "resqiar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resqiar/writeloop-pptr/issues"
  },
  "homepage": "https://github.com/resqiar/writeloop-pptr#readme",
  "dependencies": {
    "dotenv": "^16.0.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.10.0",
    "fastify-cli": "^2.15.0",
    "fastify-cors": "^6.0.2",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.2",
    "puppeteer": "^13.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.15",
    "tap": "^15.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
